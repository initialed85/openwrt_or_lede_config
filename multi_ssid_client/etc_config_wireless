config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/ar933x_wmac'
	option htmode 'HT20'
	option disabled '0'
	option channel 'auto'
	option country 'AU'

config wifi-iface
	option network 'wwan0'
	option ssid 'SSID_1'
	option encryption 'psk2'
	option device 'radio0'
	option mode 'sta'
	option key 'Password1'

config wifi-iface
	option network 'wwan1'
	option ssid 'SSID_2'
	option encryption 'psk2'
	option device 'radio0'
	option mode 'sta'
	option key 'Password2'
